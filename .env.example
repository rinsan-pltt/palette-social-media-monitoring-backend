# Example environment variables for palette-social-media-monitoring-backend

# API Keys
OPENAI_API_KEY=your-openai-api-key-here
GOOGLE_AI_API_KEY=your-google-api-key-here

# Database Config

MONGO_URI=mongodb_uri_here
MONGO_DB=pltt_social_media_monitoring
MONGO_SESSIONS_COLLECTION=sessions
MONGO_INSTAGRAM_COLLECTION=instagram
MONGO_TWITTER_COLLECTION=twitter
MONGO_FACEBOOK_COLLECTION=facebook
MONGO_YOUTUBE_COLLECTION=youtube


# Instagram credentials (optional if using DB-backed cookies)
IG_USER=your_ig_username
IG_PASSWORD=your_ig_password

# Twitter/X credentials (optional if using DB-backed cookies)
TW_USER=your_twitter_username_or_handle
TW_PASSWORD=your_twitter_password

# Facebook credentials (optional)
FB_EMAIL=your_facebook_email
FB_PASSWORD=your_facebook_password

